---
import Layout from "../layouts/Layout.astro";
import Hero from "../components/Hero.astro";
import Welcome from "../components/Welcome.astro";
import Zeiten from "../components/Zeiten.astro";
import Merkmale from "../components/Merkmale.tsx";
import Testimonials from "../components/Testimonials.tsx";
import Karte from "../components/Karte.astro";
let title = "Astro Portfolio W52s #4";
let description = "Astro Portfolio W52s #4";
const permalink = Astro.site.href;
let canonicalURL = Astro.request.canonicalURL;
---

<Layout {title} {description} {permalink} current="index" {canonicalURL}>
  <main id="main-content" aria-label="main">
    <Hero />
    <Welcome />
    <Zeiten />
    <Merkmale />
    <Testimonials client:visible />
    <Karte />
  </main>
</Layout>
